pipeline {
   agent any
   
   stages {
       stage ('Complile stage') {
           steps {
               withMaven(maven : 'maven_3.6.3')
               echo "mvn compile"
                  }
                                }
           }
         stage ('Testing stage') {
            steps {
                 withMaven(maven: 'maven_3.6.3')
                 echo "mvn test"
                  }
                                 }
              }
             }
